    <options applyeffects="yes"/>

    <code>var f = 0.001*(cos(5*t));
y  = 1.0*(saw((1.00+f)*0.1*w*t,1)-0.5);
y += 0.7*(saw((2.01+f)*0.1*w*t,1)-0.5);
y += 0.5*(saw((4.02+f)*0.1*w*t,1)-0.5);
y += 0.2*(saw((8.02+f)*0.1*w*t,1)-0.5);
y *= 20*t*exp(-4*t);
y *= 0.9+0.1*cos(40*t);</code>

